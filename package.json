{
  "name": "my-turborepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "latest",
    "@serverless-stack/cli": "1.15.16",
    "@serverless-stack/core": "1.15.16",
    "@serverless-stack/resources": "1.15.16",
    "@serverless-stack/static-site-env": "1.15.16",
    "@types/node": "^14.18.21",
    "aws-cdk-lib": "2.39.1",
    "chalk": "^4.0.0",
    "constructs": "10.1.142",
    "fs-extra": "^10.1.0",
    "typescript": "^4.8.3"
  },
  "pnpm": {
    "overrides": {
      "constructs": "10.1.142",
      "aws-cdk-lib": "2.39.1"
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "pnpm@7.14.0"
}
